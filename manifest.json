{
	"name": "besk-tab",
	"version": "0.0.1",
	"description": "A better new tab page",
	"permissions": [
		"bookmarks",
	    "file:///*",
	    "storage",
	    "https://query.yahooapis.com/"
	],
	"manifest_version": 2,
	"browser_action": {
		"default_title": "New Tab",
		//"default_icon": "icon.png",
		"default_popup": "besk-tab.html"
	},
	"chrome_url_overrides" : {
		"newtab": "besk-tab.html"
	},
	"content_security_policy": "script-src 'self' https://query.yahooapis.com/; object-src 'self'"
}